              alt="">
          </div>
          <div class="content1">
            <div class="inner-contaner">
              <div class="vbr"></div>
              <div class="first-link">
                <form action="/uploaded_posts" method="post">
                  <style>
                    button {
                      background: none;
                      border: none;
                      outline: none;
                      box-shadow: none;
                    }
                  </style>
                  <button name="list_type" value="contractors">المشاريع المطروحة</button>
                </form>
              </div>
              <div class="first-arow">
                <a href="">
                  <img src="https://github.com/mshraky3/base/blob/main/right-arrow.png?raw=true" width="60px" alt="">
                </a>
              </div>
              <% if(locals.type==="user" || locals.type==="admin" ){ %>
                <div class="sc-link">
                  <form action="/upload_post" method="post">
                    <button name="upload_post" value="<%=username%>" >
                      اعرض مشروعك
                    </button>
                  </form>
                </div>
                <div class="sc-arow">
                  <a href="">
                    <img src="https://github.com/mshraky3/base/blob/main/right-arrow.png?raw=true" width="60px" alt="">
                  </a>
                </div>
                <% }else{ %>
                  <div class="sc-link"></div>
                  <div class="sc-arow">
