              alt=""
              width="100%">
          </div>
          <div class="content1">
            <div class="inner-contaner">
              <div class="vbr"></div>
              <div class="first-link">
                <form action="/uploaded_posts" method="post">
                  <style>
                    button {
                      background: none;
                      border: none;
                      outline: none;
                      box-shadow: none;
                    }
                  </style>
                  <button name="list_type" value="freelancers">المشاريع المطروحة</button>
