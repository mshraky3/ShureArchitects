                border: 1px solid;
                width: 100%;
                height: 250px;
                margin-top: -25%;
                background-color: rgb(253, 253, 253);
                
            }
            .content1:hover{
                transform: scale(1.1); 
                transition:  margin-top 2s ease, transform 3s ease;
            }
            .content2:hover{
                transform: scale(1.1); 
                transition:  margin-top 2s ease, transform 3s ease;
            }
            .content3:hover{
                transform: scale(1.1); 
                transition:  margin-top 2s ease, transform 3s ease;
            }

            .futr-img{
                width:100%;
            }
            .content-parent
            {
                display: grid;
                grid-template-columns: repeat(2, 1fr);
                grid-template-rows: repeat(2, 1fr);
                grid-column-gap: 0px;
                grid-row-gap: 0px;
            }
            .inner-contaner{
                width: 100%;
                height: 100%;
                display: grid;
                grid-template-columns: repeat(3, 1fr) repeat(2, 0.1fr) repeat(3, 1fr);
                grid-template-rows: repeat(6, 1fr);
                grid-column-gap: 0px;
                grid-row-gap: 0px;
            }

            .vbr { grid-area: 1 / 5 / 7 / 6; background-color: black;}
            .first-link  { grid-area: 2 / 2 / 3 / 3; }
            .first-arow{ grid-area: 3 / 2 / 4 / 3; }
            .sc-link { grid-area: 4 / 2 / 5 / 3; }
            .sc-arow { grid-area: 5 / 2 / 6 / 3; }
            .main-link{ grid-area: 3 / 7 / 5 / 8;  }
            .main-link a {
